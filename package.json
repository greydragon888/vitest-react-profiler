{
  "name": "vitest-react-profiler",
  "version": "1.5.0",
  "description": "Performance testing utilities for React components and hooks with sync/async update tracking in Vitest",
  "keywords": [
    "vitest",
    "react",
    "testing",
    "profiler",
    "performance",
    "hooks",
    "async",
    "render",
    "benchmark",
    "matchers",
    "react-hooks",
    "performance-testing",
    "render-tracking",
    "component-testing",
    "hook-testing"
  ],
  "homepage": "https://github.com/greydragon888/vitest-react-profiler#readme",
  "bugs": {
    "url": "https://github.com/greydragon888/vitest-react-profiler/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/vitest-react-profiler.git"
  },
  "license": "MIT",
  "author": {
    "name": "Oleg Ivanov",
    "email": "greydragon888@gmail.com",
    "url": "https://github.com/greydragon888"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": [
    "./src/matchers/index.ts",
    "./src/auto-setup.ts",
    "./dist/index.js",
    "./dist/index.cjs"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:mutation": "stryker run",
    "test:mutation:report": "open reports/mutation-report.html",
    "test:properties": "NODE_OPTIONS='--max-old-space-size=4096' vitest run --config vitest.config.properties.mts",
    "test:bench": "vitest bench --config vitest.config.bench.mts",
    "test:bench:stable": "NODE_OPTIONS='--expose-gc --max-old-space-size=16384' vitest bench --config vitest.config.bench.mts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "bundle:analyze": "npm run build && npx @codecov/bundle-analyzer ./dist --bundle-name=vitest-react-profiler",
    "bundle:size": "npm run build && du -sh dist/* | sort -h",
    "sonar": "dotenv -- sonar-scanner -Dsonar.login=$SONAR_TOKEN",
    "sonar:debug": "dotenv -- sonar-scanner -Dsonar.login=$SONAR_TOKEN -Dsonar.log.level=DEBUG -Dsonar.verbose=true",
    "sonar:local": "npm run typecheck && npm run test:coverage && npm run sonar",
    "prepare": "node -e \"try { require('husky') } catch (e) {}\""
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vitest": ">=1.0.0"
  },
  "devDependencies": {
    "@codecov/bundle-analyzer": "1.9.1",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/js": "9.36.0",
    "@fast-check/vitest": "0.2.3",
    "@stryker-mutator/core": "9.3.0",
    "@stryker-mutator/typescript-checker": "9.3.0",
    "@stryker-mutator/vitest-runner": "9.3.0",
    "@stylistic/eslint-plugin": "5.4.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.5.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitest/coverage-v8": "4.0.8",
    "@vitest/eslint-plugin": "1.4.2",
    "dotenv-cli": "11.0.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.1.9",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-unicorn": "62.0.0",
    "fast-check": "4.3.0",
    "husky": "9.1.7",
    "jsdom": "27.1.0",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonarqube-scanner": "4.3.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vitest": "4.0.8"
  },
  "workspaces": [
    "examples/*"
  ]
}
