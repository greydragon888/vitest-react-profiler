{
  "name": "vitest-react-profiler",
  "version": "1.1.0",
  "description": "React component render tracking and performance testing utilities for Vitest",
  "keywords": [
    "vitest",
    "react",
    "testing",
    "profiler",
    "performance"
  ],
  "homepage": "https://github.com/greydragon888/vitest-react-profiler#readme",
  "bugs": {
    "url": "https://github.com/greydragon888/vitest-react-profiler/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/vitest-react-profiler.git"
  },
  "license": "MIT",
  "author": {
    "name": "Oleg Ivanov",
    "email": "greydragon888@gmail.com",
    "url": "https://github.com/greydragon888"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": [
    "./src/matchers.ts",
    "./src/auto-setup.ts",
    "./dist/index.js",
    "./dist/index.cjs"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vitest watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:watch": "vitest watch --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "prepare": "node -e \"try { require('husky') } catch (e) {}\""
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vitest": ">=1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/js": "9.36.0",
    "@stylistic/eslint-plugin": "5.4.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.5.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.13",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-sonarjs": "3.0.5",
    "husky": "9.1.7",
    "jsdom": "27.0.0",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vitest": "3.2.4"
  },
  "workspaces": [
    "examples/*"
  ]
}
